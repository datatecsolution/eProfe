<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".SincronizarSace"
    tools:showIn="@layout/activity_sincronizar_sace">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btnSincronizarBD"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Sincronizar BD" />

        <Button
            android:id="@+id/btnSincronizarSACE"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Sincronizar SACE" />

        <Button
            android:id="@+id/btnDatosPruebas"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="SET DATOS DE PRUEBA" />

        <ListView
            android:id="@+id/lvSecciones"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="4dp"
            android:padding="5dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp" >

        </ListView>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>